                    }
                ],
                "volumes": [
                    {
                        "name": "d9eb6b3c-6fc0-510b-81c0-839f2861232c-volume",
                        "configMap": {
                            "items": [
                                {
                                    "key": "fd9d0ef0db62369a389fe5abd0e8103402cbc4b048f97f398514ce173b7f6bf3_sk",
