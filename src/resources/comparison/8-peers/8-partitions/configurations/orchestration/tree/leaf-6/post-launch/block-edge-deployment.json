                    },
                    {
                        "name": "672c2a8a-71a2-5484-aca0-d001fa484947-volume",
                        "configMap": {
                            "items": [
                                {
                                    "key": "Admin-org1-cert.pem",
                                    "path": "Admin@org1-cert.pem"
                                }
                            ],
                            "name": "672c2a8a-71a2-5484-aca0-d001fa484947",
                            "optional": false
                        }
                    },
                    {
                        "name": "21b01815-3f3b-58b5-87e7-b61acebd8c22-volume",
